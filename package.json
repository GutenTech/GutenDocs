{
  "name": "gutendocs",
  "version": "0.0.2",
  "description": "API Generator Using JSDoc Parser",
  "main": "index.js",
  "bin": {
    "gutendocs": "./bin/cli.js",
    "gutenDocs": "./bin/cli.js"
  },
  "scripts": {
    "lint": "eslint \"./**/*.js?\"",
    "watchTest": "set NODE_ENV=test&& jest --watch",
    "test": "jest --maxWorkers=4 --detectOpenHandles",
    "build-dev": "webpack -p --progress",
    "react-dev": "webpack --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GutenTech/GutenDocs.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GutenTech/GutenDocs/issues"
  },
  "homepage": "https://github.com/GutenTech/GutenDocs#readme",
  "dependencies": {
    "acorn": "^5.7.1",
    "doctrine": "^2.1.0",
    "glob": "^7.1.2",
    "klaw": "^3.0.0",
    "node-dir": "^0.1.17",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "jest": "23.2.0",
    "json-loader": "^0.5.7",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test_setup.js",
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "syntax-dynamic-import"
    ]
  }
}
