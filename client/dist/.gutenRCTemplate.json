{
  "apiDir": "GutenApi/",
  "skeleton": {
    "sortByOrder": [
      "sortByFileName",
      "sortBySection",
      "sortByParentDirectoryName"
    ],
    "sortBySection": {
      "section": "@section"
    },
    "sortByFileName": {
      "includeExtension": false
    },
    "sortByParentDirectoryName": {
      "targetDepth": 1
    },
    "catchAll": {
      "section": "Miscellaneous"
    }
  },
  "customSorters": {
    "sorterName": {
      "sorterRelPath": "CustomSorters/sorterName.js",
      "sorterSpecs": {
        "someSpecOne": "foo"
      }
    },
    "sorterNameTwo": {
      "sorterRelPath": "CustomSorters/sorterNameTwo.js",
      "sorterSpecs": {
        "someSpecOne": "bar"
      }
    }
  },
  "acornSettings": {
    "allowImportExportEverywhere": true,
    "allowHashBang": true
  },
  "doctrineSettings": {
    "unwrap": true,
    "recoverable": true,
    "sloppy": true
  },
  "verbosity": 1
}